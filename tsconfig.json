{
  "compilerOptions": {
    "noImplicitAny": false,
    "noEmitOnError": true,
    "removeComments": false,
    "sourceMap": true,
    "target": "es5",
    "types": [ "node" ],
    "typeRoots": [ "../node_modules/@types" ]
  },
  "include": [
    "./app/**/*.ts",
    "./app/**/*.scss",
    "./app/**/*.html",
    "./node_modules/@types/**/*.d.ts",
    "./node_modules/@uirouter/core/lib/index.d.ts"
  ]
}
