{
  "name": "angular-webpack",
  "version": "2.1.0",
  "description": "Skeleton project using Angular 1.5.10, typescript and webpack",
  "main": "app/index.js",
  "repository": "https://github.com/deonclem/angular-webpack-typescript.git",
  "dependencies": {
    "angular": "~1.5.10",
    "angular-animate": "~1.5.10",
    "angular-aria": "~1.5.10",
    "angular-i18n": "~1.5.10",
    "angular-material": "^1.1.3",
    "angular-messages": "~1.5.10",
    "angular-ui-router": "~1.0.0-rc.1"
  },
  "devDependencies": {
    "@types/angular": "^1.5.23",
    "@types/angular-material": "^1.1.45",
    "@types/angular-mocks": "^1.5.9",
    "@types/angular-ui-router": "^1.1.36",
    "@types/jasmine": "^2.5.43",
    "@types/webpack": "^2.2.7",
    "@types/webpack-env": "^1.13.0",
    "angular-mocks": "~1.5.10",
    "autoprefixer": "^6.3.6",
    "babel-core": "^6.21.0",
    "babel-loader": "^6.2.10",
    "babel-preset-latest": "^6.16.0",
    "css-loader": "^0.26.1",
    "extract-text-webpack-plugin": "^2.1.0",
    "file-loader": "^0.9.0",
    "jasmine-core": "^2.5.2",
    "karma": "^1.3.0",
    "karma-jasmine": "^1.1.0",
    "karma-phantomjs-launcher": "^1.0.2",
    "karma-spec-reporter": "0.0.26",
    "karma-webpack": "^1.8.1",
    "ng-annotate-loader": "^0.2.0",
    "node-sass": "^4.1.1",
    "phantomjs-prebuilt": "^2.1.7",
    "postcss-loader": "^1.2.1",
    "precss": "^1.4.0",
    "raw-loader": "^0.5.1",
    "sass-loader": "^4.1.1",
    "style-loader": "^0.13.1",
    "ts-loader": "^1.3.3",
    "typescript": "^2.4.2",
    "webpack": "^2.2.1",
    "webpack-dev-server": "^2.6.1"
  },
  "scripts": {
    "test": "karma start",
    "start": "npm install && npm run-script dev",
    "dev": "node node_modules/webpack-dev-server/bin/webpack-dev-server --env.dev --content-base app --host localhost --no-inline",
    "build": "npm test && node node_modules/webpack-dev-server/bin/webpack-dev-server --env.prod -p && cp app/index.html dist/index.html",
    "dist": "npm test && rm -rf dist/ && node node_modules/webpack-dev-server/bin/webpack-dev-server --env.prod -p && cp app/index.html dist/index.html && node node_modules/webpack-dev-server/bin/webpack-dev-server --env.prod -p --compress --content-base dist --host localhost --no-inline"
  },
  "author": "deonclem",
  "license": "ISC"
}
